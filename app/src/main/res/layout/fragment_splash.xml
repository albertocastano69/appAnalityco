<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fragments.SplashFragment">

    <co.tecno.sersoluciones.analityco.views.AnimatedSvgView
        android:id="@+id/animated_svg_view"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_gravity="center"
        android:layout_marginBottom="25dp"
        app:animatedSvgFillColors="@array/analityco_glyph_colors"
        app:animatedSvgGlyphStrings="@array/analityco_glyph_strings"
        app:animatedSvgFillStart="1200"
        app:animatedSvgFillTime="1500"
        app:animatedSvgImageSizeX="644"
        app:animatedSvgImageSizeY="644"
        app:animatedSvgTraceTime="1000"
        app:animatedSvgTraceTimePerGlyph="1000"/>

</FrameLayout>
